{
  "name": "graphql-docker",
  "version": "1.0.0",
  "description": "Forgetful app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "server": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacaScheer/Forgetful.git"
  },
  "author": "Cameron Farina, Paul Choi, Anthony Chan, Michael Scheer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MacaScheer/Forgetful/issues"
  },
  "homepage": "https://github.com/MacaScheer/Forgetful#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.11",
    "nodemon": "^1.19.1",
    "react-calendar": "^2.19.1",
    "react-datepicker": "^2.9.6",
    "validator": "^11.1.0"
  }
}
